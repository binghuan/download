{
  "name": "Pizza Finder",
  "description": "Find you pizza nearby you",
  "manifest_version": 2,
  "version": "1.0.1", 
  "app": {
    "urls": [
      "*://binghuan.herokuapp.com/pizzaFinder.html"
    ],
    "launch": {
      "web_url": "http://binghuan.herokuapp.com/pizzaFinder.html"
    }
  },
  "icons": {
    "128": "Icon-128.png"
  },
  "permissions": [
    "unlimitedStorage",
    "geolocation"
  ]
}